<style>
#result_table {
	border: 1px solid black;
	border-width: 1px 1px 0 0;
	margin: 15px 0;
	border-collapse:collapse
}
#result_table td, #result_table th {
	border: 1px solid black;
	border-width: 0 0 1px 1px;
	padding: 5px 8px;
	vertical-align:top;
}
#result_table th {
	background-color: #cfc;
}
.currency {
	text-align:right;
}
.right, .rpad2 {
	text-align:right;
}
.center {
	text-align:center;
}
</style>
<table id='result_table'>
{{#Rows}}{{#first}}
	<tr class='header'>
		{{#values}}
			<th class="{{class}}">{{key}}</th>
		{{/values}}
	</tr>
{{/first}}
	<tr>
	{{#values}}
		{{#value}}<td class="{{class}}">{{#raw}}{{{value}}}{{/raw}}{{^raw}}{{#pre}}<pre>{{value}}</pre>{{/pre}}{{^pre}}{{value}}{{/pre}}{{/raw}}</td>{{/value}}
		{{^value}}<td class="{{class}}">{{value}}&nbsp;</td>{{/value}}
		
	{{/values}}
	</tr>
{{/Rows}}
</table>
